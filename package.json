{
  "name": "@bpanel/bpanel-utils",
  "version": "0.0.9",
  "description": "Utilities for bpanel ",
  "keywords": [
    "bpanel",
    "bcoin"
  ],
  "license": "MIT",
  "repository": "git://github.com/bpanel-org/bpanel-utils.git",
