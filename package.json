  "bugs": {
    "url": "https://github.com/bcoin-org/bpanel-utils/issues"
  },
  "author": "Buck Perley <bucko.perley@gmail.com>",
  "main": "./dist/index.js",
  "scripts": {
    "babel": "babel lib -d dist",
    "browserify": "browserify -s index lib/index.js | uglifyjs -c > index.js",
    "clean": "rm -f ./dist/*.js",
